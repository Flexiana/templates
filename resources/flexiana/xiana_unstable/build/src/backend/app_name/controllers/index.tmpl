(ns {{main/ns}}.controllers.index
  (:require
    [ring.util.response :as ring]))


(defn handle-index
  [state]
  (assoc state
         :response
         (ring/response "Index page")))

(defn handle-post
  [state]
  (let [data (get-in state [:request :body-params])]
    (assoc state
           :response
           (ring/response data))))
