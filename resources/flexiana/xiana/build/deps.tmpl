{:paths ["src" "resources"]
 :deps  {org.clojure/clojure         {:mvn/version "1.11.1"}
         com.flexiana/framework      {:mvn/version "0.5.0-rc3"}
         piotr-yuxuan/closeable-map  {:mvn/version "0.36.2"}
         org.clojure/tools.namespace {:mvn/version "1.3.0"}
         thheller/shadow-cljs        {:mvn/version "2.20.2"}
         com.fzakaria/slf4j-timbre   {:mvn/version "0.3.21"}
         reagent                     {:mvn/version "0.10.0"}
         re-frame                    {:mvn/version "1.1.2"}}
 :aliases
 {:run-m {:main-opts ["-m" "{{main}}.core"]}
  :run-x {:ns-default {{main}}.core
          :exec-fn    greet
          :exec-args  {:name "Clojure"}}
  :build {:deps       {io.github.clojure/tools.build
                       {:git/tag "v0.9.2" :git/sha "fe6b140"}}
          :ns-default build}
  :test  {:extra-paths ["test"]
          :extra-deps  {org.clojure/test.check {:mvn/version "1.1.1"}
                        io.github.cognitect-labs/test-runner
                        {:git/tag "v0.5.1" :git/sha "dfb30dd"}}}}}
